	wait(0.1)
	game.Lighting.Blur.Size = 35
	ImageLabel.Transparency = 0.2
	ImageButton.BackgroundTransparency = 0.2
	TextLabel.TextTransparency = 0.2
	TextLabel.Transparency = 0.2
	TextLabel_2.TextTransparency = 0.2
	wait(0.1)
	game.Lighting.Blur.Size = 30
	ImageLabel.Transparency = 0.3
	ImageButton.BackgroundTransparency = 0.3
	TextLabel.TextTransparency = 0.3
	TextLabel.Transparency = 0.3
	TextLabel_2.TextTransparency = 0.3
	wait(0.1)
	game.Lighting.Blur.Size = 25
	ImageLabel.Transparency = 0.4
	ImageButton.BackgroundTransparency = 0.4
	TextLabel.TextTransparency = 0.4
	TextLabel.Transparency = 0.4
	TextLabel_2.TextTransparency = 0.4
	wait(0.1)
	game.Lighting.Blur.Size = 20
	ImageLabel.Transparency = 0.5
	ImageButton.BackgroundTransparency = 0.5
	TextLabel.TextTransparency = 0.5
	TextLabel.Transparency = 0.5
	TextLabel_2.TextTransparency = 0.5
	wait(0.1)
	game.Lighting.Blur.Size = 15
	ImageLabel.Transparency = 0.6
	ImageButton.BackgroundTransparency = 0.6
	TextLabel.TextTransparency = 0.6
	TextLabel.Transparency = 0.6
	TextLabel_2.TextTransparency = 0.6
	wait(0.1)
	game.Lighting.Blur.Size = 10
	ImageLabel.Transparency = 0.7
	ImageButton.BackgroundTransparency = 0.7
	TextLabel.TextTransparency = 0.7
